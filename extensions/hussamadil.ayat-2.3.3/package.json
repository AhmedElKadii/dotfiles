	"main": "./extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Ayat",
			"properties": {
				"ayat.language": {
					"type": "string",
					"description": "Select preferable language",
					"default": "Arabic",
					"order": 1,
					"enum": [
						"Arabic",
						"English"
					]
				},
				"ayat.repeatedEveryMinute": {
					"type": "number",
					"default": 30,
					"order": 2,
					"enum": [
						10,
						15,
						30,
						60,
						90,
						120,
						180
					],
					"description": "Select time between ayat notification."
				},
				"ayat.getSuraName": {
					"type": "boolean",
					"order": 3,
					"description": "Get Sura Name with Aya",
					"default": true
				}
			}
		}
	},
